<div class="login-wall panel panel-default">
    {{ form_start(form, { 'action': path('fos_user_resetting_reset', {'token': token}), 'attr': { 'class': 'fos_user_resetting_reset form-group fos_user_resetting_reset' } }) }}
        {{ form_errors(form) }}

        <div class="form-group">
            {{ form_row(form.plainPassword.first, { 'attr': {'class': 'form-control'} }) }}
        </div>
        <div class="form-group">
            {{ form_row(form.plainPassword.second, { 'attr': {'class': 'form-control'} }) }}
        </div>

        {{ form_rest(form) }}

        <div class="form-group text-center">
            <input type="submit" value="{{ 'resetting.reset.submit'|trans({}, 'FOSUserBundle') }}" class="btn btn-default" />
        </div>
    {{ form_end(form) }}
</div>
