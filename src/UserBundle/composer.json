{
    "name": "carcel/user-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "A ready to use UserBundle",
    "keywords": ["User management"],
    "homepage": "https://github.com/damien-carcel/user-bundle-dev",
    "authors": [
        {
            "name": "Damien Carcel",
            "email": "damien.carcel@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Carcel\\Bundle\\UserBundle\\": "src"
        }
    },
    "require": {
        "php": ">=5.6",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "symfony/framework-bundle": "^3.2",
        "symfony/translation": "^3.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "friendsofsymfony/user-bundle": "v2.0.0-beta1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.11",
        "phpmd/phpmd": "^2.4",
        "phpspec/phpspec": "^3.0",
        "phpunit/phpunit": "^5.4",
        "squizlabs/php_codesniffer": "^2.5"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
